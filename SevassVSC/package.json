{
  "name": "sevassvsc",
  "displayName": "Sevass",
  "description": "Sevass language support for Visual Studio Code.",
  "version": "0.0.6",
  "publisher": "Salmon",
  "icon": "icons/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/LiterallyFabian/sevass"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Language Packs"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sevass",
        "aliases": [
          "Sevass",
          "sevass"
        ],
        "extensions": [
          ".cv"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/svlogo.png",
          "dark": "./icons/svlogo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "sevass",
        "scopeName": "source.cs",
        "path": "./syntaxes/sevass.tmLanguage.json"
      }
    ]
  }
}